        <!-- –––––––––––––––[ PAGE CONTENT ]––––––––––––––– -->
        <main id="mainContent" class="main-content">
            <div class="page-container ptb-10">
                <div class="container">
                    <div class="section deals-header-area ptb-30">
                        <div class="row row-tb-20">
                            <div class="col-xs-12 col-md-4 col-lg-3">
                                <aside>
                                    <ul class="nav-coupon-category panel">
                                        {{#each category}}
                                        <li><a href="/DoiTac?MaNganh={{MaNganh}}"><i class="fa fa-bars"></i>{{TenNganh}}</a>
                                        </li>
                                        {{/each}}
                                        <li class="all-cat">
                                            <a class="font-14" href="#"></a>
                                        </li>
                                    </ul>
                                </aside>
                            </div>
                            <div class="col-xs-12 col-md-8 col-lg-9">
                                <div class="header-deals-slider flexslider" id="header-deals-slider">
                                    <ul class="slides">
                                        {{#each ChienDich}}
                                        <li>
                                            <div class="deal-single panel item">
                                                <figure class="deal-thumbnail embed-responsive embed-responsive-16by9" data-bg-img="/assets/images/ChienDich/{{imgPathChienDich}}">
                                                    <div class="label-discount top-10 right-10">%</div>

                                                    <div class="deal-about p-20 pos-a bottom-0 left-0">
                       
                                                        <h3 class="deal-title mb-10 ">
                                                            <a href="/ChienDich/{{MaChienDich}}" class="color-light color-h-lighter">{{TieuDe}}</a>
                                                        </h3>
                                                    </div>
                                                </figure>
                                            </div>
                                        </li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="section explain-process-area ptb-30">
                        <div class="row row-rl-10">
                            <div class="col-md-4">
                                <div class="item panel prl-15 ptb-20">
                                    <div class="row row-rl-5 row-xs-cell">
                                        <div class="col-xs-4 valign-middle">
                                            <img class="pr-10" src="assets/images/icons/tablet.png" alt="">
                                        </div>
                                        <div class="col-xs-8">
                                            <h5 class="mb-10 pt-5">Deals & Coupons</h5>
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="item panel prl-15 ptb-20">
                                    <div class="row row-rl-5 row-xs-cell">
                                        <div class="col-xs-4 valign-middle">
                                            <img class="pr-10" src="assets/images/icons/online-shop-6.png" alt="">
                                        </div>
                                        <div class="col-xs-8">
                                            <h5 class="mb-10 pt-5">Find Best Offers</h5>
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="item panel prl-15 ptb-20">
                                    <div class="row row-rl-5 row-xs-cell">
                                        <div class="col-xs-4 valign-middle">
                                            <img class="pr-10" src="assets/images/icons/money.png" alt="">
                                        </div>
                                        <div class="col-xs-8">
                                            <h5 class="mb-10 pt-5">Save Money</h5>
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <section class="section latest-deals-area ptb-30">
                        <header class="panel ptb-15 prl-20 pos-r mb-30">
                            <h3 class="section-title font-18">Chiến Dịch</h3>
                            <a class="btn btn-o btn-xs pos-a right-10 pos-tb-center" href="/ChienDich?grid=1">View All</a>
                        </header>

                        <div class="row row-masnory row-tb-20 owl-slider" data-loop="true" data-autoplay="true" data-smart-speed="1000" data-autoplay-timeout="10000" data-margin="20" data-items="2" data-xxs-items="2" data-xs-items="2" data-sm-items="3" data-md-items="3" data-lg-items="3">
                            {{#each ChienDich}}
                            <div class="col-sm-6 col-lg-12">
                                <div class="deal-single panel">
                                    <figure class="deal-thumbnail embed-responsive embed-responsive-16by9" data-bg-img="/assets/images/ChienDich/{{imgPathChienDich}}">
                                        <div class="label-discount right-15 top-15">%</div>
                                        <ul class="deal-actions top-15 right-20">
                                            <li class="like-deal">
                            
                                            </li>
                                        </ul>
                                        <div class="time-left bottom-15 right-20 font-md-14">
                                            <span>
                                        </div>
                            
                                    </figure>
                                    <div class="bg-white pt-20 pl-20 pr-15">
                                        <div class="pr-md-10">
                                            <div class="rating mb-10">
                            
                                            </div>
                                            <h3 class="deal-title mb-10">
                                                <a href="/ChienDich/{{MaChienDich}}">{{TieuDe}}</a>
                                            </h3>
                                            <ul class="deal-meta list-inline mb-10 color-mid">
                                                <li><i class="ico fa fa-map-marker mr-10"></i>Thời gian bắt đầu: {{ThoiGianBatDau}}</li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                    </section>


                    <section class="section latest-coupons-area ptb-30">
                        <header class="panel ptb-15 prl-20 pos-r mb-30">
                            <h3 class="section-title font-18">Voucher Mới Nhất</h3>
                            <a class="btn btn-o btn-xs pos-a right-10 pos-tb-center" href="/vouchers">View All</a>
                        </header>

                        <div class="latest-coupons-slider owl-slider" data-autoplay-hover-pause="true" data-loop="true" data-autoplay="true" data-smart-speed="1000" data-autoplay-timeout="10000" data-margin="30" data-nav-speed="false" data-items="1" data-xxs-items="1" data-xs-items="2" data-sm-items="2" data-md-items="3" data-lg-items="4">
                            {{#each NewVoucher}}
                            <div class="coupon-item">
                                <div class="coupon-single panel t-center">
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="text-center p-20">
                                                <img class="store-logo" src="{{imgPath}}" alt="">
                                            </div>
                                            <!-- end media -->
                                        </div>
                                        <!-- end col -->
                            
                                        <div class="col-xs-12">
                                            <div class="panel-body">
                                                <ul class="deal-meta list-inline mb-10">
                                                    <h2 class="color-green"><Strong>VOUCHER</Strong></h2>
                                                </ul>
                                                <h4 class="color-green mb-10 t-uppercase">DISCOUNT {{TiLeGiamGia}}% </h4>
                                                <h6 class="deal-title mb-10">
                                                    <a href="#">{{TenDoiTac}}</a>
                                                </h6>
                                                <div class=" color-muted  font-6" style="size: 3rem;"><i class="lnr lnr-clock mr-10"></i>Ngày Tạo: {{ThoiHanTuFormat}}</div>
                                                <div class="mb-15 color-muted mb-20 font-6"><i class="lnr lnr-clock mr-10"></i>Ngày hết hạn:
                                                    {{ThoiHanDenFormat}}</div>
                                                
                                            </div>
                                        </div>
                                        <!-- end col -->
                                    </div>
                                    <!-- end row -->
                                </div>
                            </div>
                            {{/each}}
                            
                        </div>
                    </section>
                    <section class="section stores-area stores-area-v1 ptb-30">
                        <header class="panel ptb-15 prl-20 pos-r mb-30">
                            <h3 class="section-title font-18">Đối Tác</h3>
                            <a href="/DoiTac" class="btn btn-o btn-xs pos-a right-10 pos-tb-center">view ALL</a>
                        </header>
                        <div class="popular-stores-slider owl-slider" data-loop="true" data-autoplay="true" data-smart-speed="1000" data-autoplay-timeout="10000" data-margin="20" data-items="2" data-xxs-items="2" data-xs-items="2" data-sm-items="3" data-md-items="5" data-lg-items="6">
                            {{#each DoiTac}}
                            <div class="store-item t-center">
                                <a href="/DoiTac/{{MaDoiTac}}" class="panel is-block">
                                    <div class="embed-responsive embed-responsive-4by3">
                                        <div class="store-logo">
                                            <img src="/assets/images/DoiTac/{{imgPath}}" alt="">
                                        </div>
                                    </div>
                                    <h6 class="/DoiTac/{{MaDoiTac}}">{{TenDoiTac}}</h6>
                                </a>
                            </div>
                            {{/each}}
                        </div>
                    </section>


                </div>
            </div>


        </main>
